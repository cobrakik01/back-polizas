                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
	    eg.[Id]
      , eg.[FechaDeEgreso]
      , eg.[Cantidad]
      , eg.[Descripcion]
      , eg.[MinisteriosPublicosId]
	  , mi.Nombre as NombreMinisterio
	  , mi.AutoridadId
	  , au.Nombre as NombreAutoridad
	  , eg.[DetalleUsuarioId]
	  , u.Nombre as NombreUsuario
	  , u.ApellidoMaterno as ApellidoMaternoUsuario
	  , u.ApellidoPaterno as ApellidoPaternoUsuario
	  , u.NumeroDeEmpleado as NumeroDeEmpleadoUsuario
	  , u.AreaId as AreaIdUsuario
	  , area.Nombre as NombreAreaUsuario
  FROM [Egresos] eg
  left join MinisteriosPublicos mi on mi.Id = eg.MinisteriosPublicosId
  left join Autoridades au on au.Id = mi.AutoridadId
  left join DetallesUsuarios u on u.Id = eg.DetalleUsuarioId
  left join Areas area on area.Id = u.AreaId</CommandText>
